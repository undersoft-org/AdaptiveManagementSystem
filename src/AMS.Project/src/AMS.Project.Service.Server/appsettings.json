        "ConnectionString": "http://project-service-server-stocks:20456/",
        "ClientProvider": "Open",
        "PoolSize": 16
      }
    }
  },
  "Serilog": {
    "MinimumLevel": "Information",
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:yyyy/MM/dd HH:mm:ss} {Level:u11}] {Message:lj} {NewLine}{Exception}{NewLine}",
          "theme": "Serilog.Sinks.SystemConsole.Themes.SystemConsoleTheme::Literate, Serilog.Sinks.Console"
        }
      },
      {
        "Name": "File",
        "Args": {
          "outputTemplate": "[{Timestamp:yyyy/MM/dd HH:mm:ss} {Level:u10}] {Message:lj} {NewLine}{Exception}{NewLine}",
          "path": "./service.log",
          "shared": "true",
          "fileSizeLimitBytes": "1000000",
          "rollOnFileSizeLimit": true,
          "flushToDiskInterval": "1"
        }
      }
    ]
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Protocols": "Http1",
        "Url": "http://+:20406"
      },
      "Https": {
        "Protocols": "Http1AndHttp2",
        "Url": "https://+:20405"
      }
    },
    "Certificates": {
      "Default": {
        "Path": "Properties/pem.crt",
        "KeyPath": "Properties/pem.key",
        "AllowInvalid": true
      }
    }
  },
  "StoreRoutes": {
    "ProjectSalesStore": "Data/Project/Sales",
    "ProjectRequestsStore": "Data/Project/Requests",
    "ProjectProgressStore": "Data/Project/Progress",
    "ProjectPlansStore": "Data/Project/Plans",
    "ProjectStocksStore": "Data/Project/Stocks",
    "ProjectPlansEventStore": "Bus/Project/Plans",
    "ProjectProgressEventStore": "Bus/Project/Progress",
    "ProjectRequestsEventStore": "Bus/Project/Requests",
    "ProjectSalesEventStore": "Bus/Project/Sales",
    "ProjectStocksEventStore": "Bus/Project/Stocks",
    "ProjectStore": "Data/Project",
    "ProjectEventStore": "Bus/Project",
    "EventStore": "Bus",
    "DataStore": "Data",
    "AccountStore": "Auth"
  },
  "OpenTelemetry": {
    "IncludeFormattedMessage": true,
    "IncludeScopes": true,
    "ParseStateValues": true
  },
  "ServiceName": "project-service-server",
  "UseTracingExporter": "otlp",
  "UseMetricsExporter": "prometheus",
  "UseLogExporter": "console",
  "HistogramAggregation": "explicit",
  "Otlp": {
