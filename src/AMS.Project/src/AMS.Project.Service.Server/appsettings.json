        "ConnectionString": "http://project-service-server-plans:20416/",
        "ClientProvider": "Open",
        "PoolSize": 16
      },
      "Undersoft.AMS.Project.Service.Clients.ProjectProgressClient": {
        "ConnectionString": "http://project-service-server-progress:20426/",
        "ClientProvider": "Open",
        "PoolSize": 16
