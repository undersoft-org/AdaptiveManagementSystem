{
  "General": {
    "Name": "undersoft-ams-service-application-server",
    "BaseUrl": "https://undersoft-ams-service-application-server:10005",
    "Version": "v1",
    "TypeName": "Undersoft.SDK.Service.Application.Server.Hosting.ApplicationServerHost"
  },

  "AllowedHosts": "*",

  "Repository": {
    "Sources": {
      "Undersoft.AMS.Service.Infrastructure.Stores.EntryStore": {
        "ConnectionString": "Host=undersoft-ams-data-server; Port=5432; Database=us-ams-app-entry-db; UserId=postgres; Password=Q00dkaPL; Pooling=true;",
        "SourceProvider": "PostgreSql",
        "PoolSize": 32
      },
      "Undersoft.AMS.Service.Infrastructure.Stores.EventStore": {
        "ConnectionString": "Host=undersoft-ams-data-server; Port=5432; Database=us-ams-app-event-db; UserId=postgres; Password=Q00dkaPL; Pooling=true;",
        "SourceProvider": "PostgreSql",
        "PoolSize": 32
      },
      "Undersoft.AMS.Service.Infrastructure.Stores.ReportStore": {
        "ConnectionString": "Host=undersoft-ams-data-server; Port=5432; Database=us-ams-app-report-db; UserId=postgres; Password=Q00dkaPL; Pooling=true;",
        "SourceProvider": "PostgreSql",
        "PoolSize": 32
      }
    },
    "Clients": {
      "Undersoft.AMS.Service.Clients.ServiceClient": {
        "ConnectionString": "http://undersoft-ams-service-server:20006/",
        "ClientProvider": "Open",
        "PoolSize": 32
      },
      "Undersoft.AMS.Service.Clients.AccessClient": {
        "ConnectionString": "http://undersoft-ams-service-server:20006/",
        "ClientProvider": "Open",
        "PoolSize": 32
      },
      "Undersoft.AMS.Service.Clients.LogisticClient": {
        "ConnectionString": "http://undersoft-ams-service-server:20106/",
        "ClientProvider": "Open",
        "PoolSize": 32
      },
      "Undersoft.AMS.Service.Clients.CatalogClient": {
        "ConnectionString": "http://undersoft-ams-service-server:20306/",
        "ClientProvider": "Open",
        "PoolSize": 32
      },
      "Undersoft.AMS.Service.Clients.DocumentClient": {
        "ConnectionString": "http://undersoft-ams-service-server:20206/",
        "ClientProvider": "Open",
        "PoolSize": 32
      },
      "Undersoft.AMS.Service.Clients.MarketClient": {
        "ConnectionString": "http://undersoft-ams-service-server:20406/",
        "ClientProvider": "Open",
        "PoolSize": 32
      }
    }
  },
  "DataCache": {
    "Hours": "0",
    "Minutes": "45"
  },
  "Serilog": {
    "MinimumLevel": "Information",
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:yyyy/MM/dd HH:mm:ss} {Level:u11}] {Message:lj} {NewLine}{Exception}{NewLine}",
          "theme": "Serilog.Sinks.SystemConsole.Themes.SystemConsoleTheme::Literate, Serilog.Sinks.Console"
        }
      },
      {
        "Name": "File",
        "Args": {
          "outputTemplate": "[{Timestamp:yyyy/MM/dd HH:mm:ss} {Level:u11}] {Message:lj} {NewLine}{Exception}{NewLine}",
          "path": "../../logs/test.txt",
          "shared": "true",
          "fileSizeLimitBytes": "1000000",
          "rollOnFileSizeLimit": true,
          "flushToDiskInterval": "1"
        }
      }
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information"
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Protocols": "Http1",
        "Url": "http://+:10006"
      },
      "Https": {
        "Protocols": "Http1AndHttp2",
        "Url": "https://+:15015"
      }
    }
  },
  "StoreRoutes": {
    "CatalogStore": "Data/Catalog",
    "DocumentStore": "Data/Document",
    "LogisticStore": "data/logistic",
    "MarketStore": "data/market",
    "CenterStore": "Data",
    "EntryStore": "Entry",
    "ReportStore": "Report",
    "EventStore": "Bus",
    "DataStore": "Data",
    "AccountStore": "Auth"
  },
  "OpenTelemetry": {
    "IncludeFormattedMessage": true,
    "IncludeScopes": true,
    "ParseStateValues": true
  },
  "ServiceName": "undersoft-ams-service-application-server",
  "UseTracingExporter": "jeager",
  "UseMetricsExporter": "prometheus",
  "UseLogExporter": "console",
  "HistogramAggregation": "explicit",
  "Jaeger": {
    "AgentHost": "undersoft-ams-tracing-server",
    "AgentPort": 6831,
    "Endpoint": "http://undersoft-ams-tracing-server:14268",
    "Protocol": "UdpCompactThrift"
  },
  "AspNetCoreInstrumentation": {
    "RecordException": "true"
  },
  "AccessServer": {
    "ServerBaseUrl": "http://undersoft-ams-service-server:20006",
    "ServiceName": "undersoft-ams-service-application-server",
    "ServiceVersion": "v1",
    "ServiceBaseUrl": "http://undersoft-ams-service-application-server:10006",
    "ServiceClientId": "undersoft-ams-service-application-server",
    "Audience": "http://undersoft-ams-service-server:20006",
    "Issuer": "http://undersoft-ams-service-server:20006",
    "Scopes": [
      "account.read",
      "account.update",
      "account.create",
      "account.delete",
      "account.access",
      "account.action",
      "account.setup",
