{
  "General": {
    "Name": "service-server",
    "Version": "v1",
    "BaseUrl": "http://service-server:20006",
    "TypeName": "Undersoft.SDK.Service.Server.Hosting.ServerHost",
    "ServiceId": 20000
  },
  "AllowedHosts": "*",

  "Cache": {
    "Hours": "0",
    "Minutes": "45"
  },

  "Repository": {
    "Sources": {
      "Undersoft.AMS.Service.Infrastructure.Stores.AccountStore": {
        "ConnectionString": "Host=data-server; Port=5432; Database=us-ams-account-db; UserId=postgres; Password=Q00dkaPL; Pooling=true;",
        "SourceProvider": "PostgreSql",
        "PoolSize": 32
      },
      "Undersoft.AMS.Service.Infrastructure.Stores.EventStore": {
        "ConnectionString": "Host=data-server; Port=5432; Database=us-ams-event-db; UserId=postgres; Password=Q00dkaPL; Pooling=true;",
        "SourceProvider": "PostgreSql",
        "PoolSize": 32
      }
    },
    "Clients": {
      "Undersoft.AMS.Service.Clients.ApplicationClient": {
        "ConnectionString": "http://service-application-server:10006/",
        "ClientProvider": "Open",
        "PoolSize": 16
      },
      "Undersoft.AMS.Service.Clients.LogisticClient": {
        "ConnectionString": "http://logistic-service-server:20106/",
        "ClientProvider": "Open",
        "PoolSize": 32
      },
      "Undersoft.AMS.Service.Clients.CatalogClient": {
        "ConnectionString": "http://catalog-service-server:20306/",
        "ClientProvider": "Open",
        "PoolSize": 32
      },
      "Undersoft.AMS.Service.Clients.DocumentClient": {
        "ConnectionString": "http://document-service-server:20206/",
        "ClientProvider": "Open",
        "PoolSize": 32
      },
      "Undersoft.AMS.Service.Clients.MarketClient": {
        "ConnectionString": "http://market-service-server:20406/",
        "ClientProvider": "Open",
        "PoolSize": 32
      }
    }
  },
  "Serilog": {
    "MinimumLevel": "Information",
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:yyyy/MM/dd HH:mm:ss} {Level:u11}] {Message:lj} {NewLine}{Exception}{NewLine}",
          "theme": "Serilog.Sinks.SystemConsole.Themes.SystemConsoleTheme::Literate, Serilog.Sinks.Console"
        }
      },
      {
        "Name": "File",
        "Args": {
          "outputTemplate": "[{Timestamp:yyyy/MM/dd HH:mm:ss} {Level:u10}] {Message:lj} {NewLine}{Exception}{NewLine}",
          "path": "../../logs/test.txt",
          "shared": "true",
          "fileSizeLimitBytes": "1000000",
          "rollOnFileSizeLimit": true,
          "flushToDiskInterval": "1"
        }
      }
    ]
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Protocols": "Http1",
        "Url": "http://+:20006"
      },
      "Https": {
        "Protocols": "Http1AndHttp2",
        "Url": "https://+:20005"
      }
    }
  },
  "StoreRoutes": {
    "CatalogStore": "Data/Catalog",
    "DocumentStore": "Data/Document",
    "LogisticStore": "Data/Logistic",
    "MarketStore": "Data/Market",
    "CenterStore": "Data",
    "EventStore": "Bus",
    "DataStore": "Data",
    "AccountStore": "Auth"
  },
  "OpenTelemetry": {
    "IncludeFormattedMessage": true,
    "IncludeScopes": true,
    "ParseStateValues": true
  },
  "ServiceName": "service-server",
  "UseTracingExporter": "otlp",
  "UseMetricsExporter": "prometheus",
  "UseLogExporter": "console",
  "HistogramAggregation": "explicit",
  "Otlp": {
    "Source": "http://service-server:4317/"
  },
  "AspNetCoreInstrumentation": {
    "RecordException": "true"
  },
  "AccessServer": {
    "ServerBaseUrl": "http://service-server:20006",
    "ServiceName": "service-server",
    "ServiceVersion": "v1",
    "ServiceBaseUrl": "http://service-server:20006",
    "ServiceClientId": "service-server",
    "Audience": "http://service-server:20006",
    "Issuer": "http://service-server:20006",
    "Scopes": [
      "query",
      "change",
      "changeset",
      "update",
      "updateset",
      "create",
      "createset",
      "delete",
      "deleteset",
      "access",
      "action",
      "setup"
    ],
    "Claims": [
      "group",
      "id",
      "type_id",
      "code_no",
      "email",
      "name",
      "profile",
      "label",
      "type",
      "role",
      "scope",
      "client_id",
      "user_id",
      "subscription_id",
      "subscription",
      "organization_id",
      "organization",
      "tenant_id",
      "tenant",
      "email"
    ],
    "Roles": [
      "developer",
      "member",
      "user",
      "manager",
      "personel",
      "administrator",
      "provider",
      "subcontractor",
      "customer",
      "servicer",
      "integrator",
      "service_client",
      "service_app_client",
      "service",
      "service_server",
      "service_app",
      "service_app_server",
      "operator",
      "guest"
    ],
    "AdministrationRole": "administrator",
    "RequireHttpsMetadata": false,
    "CorsAllowAnyOrigin": true,
    "CorsAllowOrigins": []
  }
}