{
  "General": {
    "Name": "catalog-service-server",
    "Version": "v1",
    "BaseUrl": "http://catalog-service-server:20306",
    "TypeName": "Undersoft.SDK.Service.Server.Hosting.ServerHost",
    "ServiceId": 20300
  },
  "AllowedHosts": "*",

  "Cache": {
    "Hours": "0",
    "Minutes": "45"
  },

  "Repository": {
    "Sources": {
      "Undersoft.AMS.Service.Infrastructure.Stores.EventStore": {
        "ConnectionString": "Host=data-server; Port=5432; Database=us-ams-catalog-event-db; UserId=postgres; Password=Q00dkaPL; Pooling=true;",
        "SourceProvider": "PostgreSql",
        "PoolSize": 32
      }
    },
    "Clients": {
      "Undersoft.AMS.Catalog.Service.Clients.ParticipantEventClient": {
        "ConnectionString": "http://catalog-service-server-participants:20316/",
        "ClientProvider": "Open",
        "PoolSize": 16
      },
      "Undersoft.AMS.Catalog.Service.Clients.ResourceEventClient": {
        "ConnectionString": "http://catalog-service-server-resources:20326/",
        "ClientProvider": "Open",
        "PoolSize": 16
      },
      "Undersoft.AMS.Catalog.Service.Clients.SubjectEventClient": {
        "ConnectionString": "http://catalog-service-server-subjects:20336/",
        "ClientProvider": "Open",
        "PoolSize": 16
      },
      "Undersoft.AMS.Catalog.Service.Clients.CatalogParticipantsClient": {
        "ConnectionString": "http://catalog-service-server-participants:20316/",
        "ClientProvider": "Open",
        "PoolSize": 32
      },
      "Undersoft.AMS.Catalog.Service.Clients.CatalogResourcesClient": {
        "ConnectionString": "http://catalog-service-server-subjects:20326/",
        "ClientProvider": "Open",
        "PoolSize": 32
      },
      "Undersoft.AMS.Catalog.Service.Clients.CatalogSubjectsClient": {
        "ConnectionString": "http://catalog-service-server-subjects:20336/",
        "ClientProvider": "Open",
        "PoolSize": 32
      }
    }
  },
  "Serilog": {
    "MinimumLevel": "Information",
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:yyyy/MM/dd HH:mm:ss} {Level:u11}] {Message:lj} {NewLine}{Exception}{NewLine}",
          "theme": "Serilog.Sinks.SystemConsole.Themes.SystemConsoleTheme::Literate, Serilog.Sinks.Console"
        }
      },
      {
        "Name": "File",
        "Args": {
          "outputTemplate": "[{Timestamp:yyyy/MM/dd HH:mm:ss} {Level:u10}] {Message:lj} {NewLine}{Exception}{NewLine}",
          "path": "./service.log",
          "shared": "true",
          "fileSizeLimitBytes": "1000000",
          "rollOnFileSizeLimit": true,
          "flushToDiskInterval": "1"
        }
      }
    ]
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Protocols": "Http1",
        "Url": "http://+:20306"
      },
      "Https": {
        "Protocols": "Http1AndHttp2",
        "Url": "https://+:20305"
      }
    },
    "Certificates": {
      "Default": {
        "Path": "Properties/pem.crt",
        "KeyPath": "Properties/pem.key",
        "AllowInvalid": true
      }
    }
  },
  "StoreRoutes": {
    "CatalogParticipantsStore": "Data/Catalog/Participants",
    "CatalogResourcesStore": "Data/Catalog/Resources",
    "CatalogSubjectsStore": "Data/Catalog/Subjects",
    "CatalogCenterStore": "Data/Catalog",
    "EntryStore": "Entry",
    "ReportStore": "Report",
    "EventStore": "Bus",
    "DataStore": "Data",
    "AccountStore": "Auth"
  },
  "OpenTelemetry": {
    "IncludeFormattedMessage": true,
    "IncludeScopes": true,
    "ParseStateValues": true
  },
  "ServiceName": "catalog-service-server",
  "UseTracingExporter": "otlp",
  "UseMetricsExporter": "prometheus",
  "UseLogExporter": "console",
  "HistogramAggregation": "explicit",
  "Otlp": {
    "Source": "http://catalog-service-server:4317/"
  }, 
  "AspNetCoreInstrumentation": {
    "RecordException": "true"
  },
  "AccessServer": {
    "ServerBaseUrl": "http://service-server:20006",
    "ServiceName": "catalog-service-server",
    "ServiceVersion": "v1",
    "ServiceBaseUrl": "http://catalog-service-server:20306",
    "ServiceClientId": "catalog-service-server",
    "Audience": "http://service-server:20006",
    "Issuer": "http://service-server:20006",
    "Scopes": [
      "query",
      "change",
      "changeset",
      "update",
      "updateset",
      "create",
      "createset",
      "delete",
      "deleteset",
      "access",
      "action",
      "setup"
    ],
    "Claims": [
      "group",
      "id",
      "type_id",
      "code_no",
      "email",
      "name",
      "profile",
      "label",
      "type",
      "role",
      "scope",
      "client_id",
      "user_id",
      "subscription_id",
      "subscription",
      "organization_id",
      "organization",
      "tenant_id",
      "tenant",
      "email"
    ],
    "Roles": [
      "developer",
      "member",
      "user",
      "manager",
      "personel",
      "administrator",
      "provider",
      "subcontractor",
      "customer",
      "servicer",
      "integrator",
      "service_client",
      "service_app_client",
      "service",
      "service_server",
      "service_app",
      "service_app_server",
      "operator",
      "guest"
    ],
    "AdministrationRole": "administrator",
    "RequireHttpsMetadata": false,
    "CorsAllowAnyOrigin": true,
    "CorsAllowOrigins": []
  }
}