{
  "General": {
    "Name": "undersoft-ams-service-server",
    "Version": "v1",
    "BaseUrl": "http://undersoft-ams-service-server:26426",
    "TypeName": "Undersoft.SDK.Service.Server.Hosting.ServerHost"
  },
  "AllowedHosts": "*",

  "Cache": {
    "Hours": "0",
    "Minutes": "45"
  },

  "Repository": {
    "Sources": {
      "Undersoft.AMS.Market.Service.Infrastructure.Stores.EventStore": {
        "ConnectionString": "Host=undersoft-ams-data-server; Port=5432; Database=us-ams-market-event-db; UserId=postgres; Password=Q00dkaPL; Pooling=true;",
        "SourceProvider": "PostgreSql",
        "PoolSize": 32
      }
    },
    "Clients": {
      "Undersoft.AMS.Market.Service.Clients.MarketConsumesClient": {
        "ConnectionString": "http://undersoft-ams-service-server:26436/",
        "ClientProvider": "Open",
        "PoolSize": 32
      },
      "Undersoft.AMS.Market.Service.Clients.MarketProducesClient": {
        "ConnectionString": "http://undersoft-ams-service-server:26446/",
        "ClientProvider": "Open",
        "PoolSize": 32
      },
      "Undersoft.AMS.Market.Service.Clients.MarketPurchasesClient": {
        "ConnectionString": "http://undersoft-ams-service-server:26456/",
        "ClientProvider": "Open",
        "PoolSize": 32
      },
      "Undersoft.AMS.Market.Service.Clients.MarketSalesClient": {
        "ConnectionString": "http://undersoft-ams-service-server:26466/",
        "ClientProvider": "Open",
        "PoolSize": 32
      },
      "Undersoft.AMS.Market.Service.Clients.MarketStocksClient": {
        "ConnectionString": "http://undersoft-ams-service-server:26476/",
        "ClientProvider": "Open",
        "PoolSize": 32
      }
    }
  },
  "Serilog": {
    "MinimumLevel": "Information",
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:yyyy/MM/dd HH:mm:ss} {Level:u11}] {Message:lj} {NewLine}{Exception}{NewLine}",
          "theme": "Serilog.Sinks.SystemConsole.Themes.SystemConsoleTheme::Literate, Serilog.Sinks.Console"
        }
      },
      {
        "Name": "File",
        "Args": {
          "outputTemplate": "[{Timestamp:yyyy/MM/dd HH:mm:ss} {Level:u10}] {Message:lj} {NewLine}{Exception}{NewLine}",
          "path": "../../logs/test.txt",
          "shared": "true",
          "fileSizeLimitBytes": "1000000",
          "rollOnFileSizeLimit": true,
          "flushToDiskInterval": "1"
        }
      }
    ]
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Protocols": "Http1",
        "Url": "http://+:26426"
      },
      "Https": {
        "Protocols": "Http1AndHttp2",
        "Url": "https://+:25425"
      }
    }
  },
  "StoreRoutes": {
    "MarketSalesStore": "data/market/sales",
    "MarketPurchasesStore": "data/market/purchases",
    "MarketProducesStore": "data/market/produces",
    "MarketConsumesStore": "data/market/consumes",
    "MarketStocksStore": "data/market/stocks",
    "MarketCenterStore": "data/market",
    "EntryStore": "entry",
    "ReportStore": "report",
    "EventStore": "",
    "DataStore": "data",
    "AccountStore": "auth"
  },
  "OpenTelemetry": {
    "IncludeFormattedMessage": true,
    "IncludeScopes": true,
    "ParseStateValues": true
  },
  "ServiceName": "undersoft-ams-service-server",
  "UseTracingExporter": "jeager",
  "UseMetricsExporter": "prometheus",
  "UseLogExporter": "console",
  "HistogramAggregation": "explicit",
  "Jaeger": {
    "AgentHost": "undersoft-ams-service-server",
    "AgentPort": 6831,
    "Endpoint": "http://undersoft-ams-tracing-server:14268",
    "Protocol": "UdpCompactThrift"
  },
  "AspNetCoreInstrumentation": {
    "RecordException": "true"
  },
  "AccessServer": {
    "ServerBaseUrl": "http://undersoft-ams-service-server:26426",
    "ServiceName": "undersoft-ams-service-server",
    "ServiceVersion": "v1",
    "ServiceBaseUrl": "http://undersoft-ams-service-server:26426",
    "ServiceClientId": "undersoft-ams-service-server",
    "Audience": "http://undersoft-ams-service-server:26426",
    "Issuer": "http://undersoft-ams-service-server:26426",
    "Scopes": [
      "account.read",
      "account.update",
      "account.create",
      "account.delete",
      "account.access",
      "account.action",
      "account.setup",
      "contact.read",
      "contact.update",
      "contact.create",
      "contact.delete",
      "contact.access",
      "contact.action",
      "contact.setup"
    ],
    "Claims": [
      "group",
      "id",
      "type_id",
      "code_no",
      "email",
      "name",
      "profile",
      "label",
      "type",
      "role",
      "scope",
      "client_id",
      "user_id",
      "subscription_id",
      "subscription",
      "organization_id",
      "organization",
      "tenant_id",
      "tenant",
      "email"
    ],
    "Roles": [
      "developer",
      "member",
      "user",
      "manager",
      "personel",
      "administrator",
      "provider",
      "subcontractor",
      "customer",
      "servicer",
      "integrator",
      "service_client",
      "service_app_client",
      "service",
      "service_server",
      "service_app",
      "service_app_server",
      "operator",
      "guest"
    ],
    "AdministrationRole": "administrator",
    "RequireHttpsMetadata": false,
    "CorsAllowAnyOrigin": true,
    "CorsAllowOrigins": []
  }
}